alias g='gvim'
alias set_small='xrandr --output DP-1 --mode 1680x1050'
alias set_full='xrandr --output DP-1 --mode 1920x1200'
alias tmux='tmux -u'
alias gitdiff='git difftool --tool=tkdiff -y'
alias gitlog="git log --graph --decorate --all --color --pretty=format:'%C(auto)%h %C(bold blue)(%cn) %C(reset)%s %C(green)%d'"
